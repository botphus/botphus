{
  "name": "botphus",
  "version": "0.1.0",
  "description": "Botphus task management website",
  "private": true,
  "directories": {
    "test": "dist/test/"
  },
  "scripts": {
    "build": "npm run lint && tsc",
    "commit": "npm run test",
    "test": "npm run build && npm run test:cover",
    "test:unit": "mocha ./dist/test/",
    "test:cover": "nyc --reporter=lcov npm run test:unit",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "nyc": {
    "exclude": [
      "dist/test/**"
    ]
  },
  "pre-commit": [
    "commit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botphus/botphus.git"
  },
  "keywords": [
    "botphus",
    "test",
    "QTP",
    "puppeteer",
    "website"
  ],
  "author": "bakajinsei <cqggff@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botphus/botphus/issues"
  },
  "homepage": "http://www.botphus.org/",
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.2.18",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "power-assert": "^1.6.0",
    "pre-commit": "^1.2.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "botphus-core": "^0.1.2",
    "fastify": "^1.12.1",
    "fs-extra": "^7.0.0",
    "mongoose": "^5.3.2",
    "mongoose-validator": "^2.1.0"
  }
}
