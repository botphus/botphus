{
  "name": "botphus",
  "version": "0.1.0",
  "description": "Botphus task management website",
  "private": true,
  "directories": {
    "test": "dist/test/"
  },
  "scripts": {
    "start": "node www",
    "build": "npm run lint && tsc",
    "commit": "npm run test",
    "test": "npm run build && npm run test:cover",
    "test:unit": "node ./test/create_config && mocha ./dist/test/ && node ./test/remove_config",
    "test:cover": "node ./test/create_config && nyc --reporter=lcov npm run test:unit && node ./test/remove_config",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "nyc": {
    "exclude": [
      "dist/test/**"
    ]
  },
  "pre-commit": [
    "commit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botphus/botphus.git"
  },
  "keywords": [
    "botphus",
    "test",
    "QTP",
    "puppeteer",
    "website"
  ],
  "author": "bakajinsei <cqggff@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botphus/botphus/issues"
  },
  "homepage": "http://www.botphus.org/",
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.1",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.2.18",
    "@types/pino": "^5.6.0",
    "@types/power-assert": "^1.5.0",
    "@types/recursive-readdir": "^2.2.0",
    "@types/serve-static": "^1.13.2",
    "@types/supertest": "^2.0.6",
    "@types/ws": "^6.0.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "power-assert": "^1.6.0",
    "pre-commit": "^1.2.2",
    "supertest": "^3.3.0",
    "tslint": "^5.11.0",
    "type-schema": "^0.1.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/serve-favicon": "^2.2.30",
    "botphus-core": "^0.1.2",
    "cookie": "^0.3.1",
    "fastify": "^1.12.1",
    "fastify-cookie": "^2.0.2",
    "fastify-helmet": "^2.1.0",
    "fastify-plugin": "^1.2.1",
    "fs-extra": "^7.0.0",
    "minimatch": "^3.0.4",
    "mongoose": "^5.3.2",
    "mongoose-validator": "^2.1.0",
    "pino": "^5.7.0",
    "pino-pretty": "^2.2.1",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2",
    "uid": "0.0.2",
    "ws": "^6.1.0"
  }
}
