// Don't change this file, please create "override.json" to override config
{
    // Server listen port
    "port": 8080,
    // Locale package name for site & antd, check detail: https://ant.design/docs/react/i18n-cn
    "locale": "zh_CN",
    // Ioredis setting
    "redis": {
        "port": 6379,
        "host": "127.0.0.1"
    },
    // Mongoose setting
    "db": "mongodb://127.0.0.1:27017/botphus",
    // Socket path, if set local, this application will run socket server
    "socket": "local",
    // Socket port, will use when socket is "local"
    "socketPort": 8110,
    // IMPORTANT: Password salt, please override those salt in "override.json"
    "serverSalt": "botphus",
    "clientSalt": "botphus-client"
}